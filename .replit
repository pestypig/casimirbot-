modules = ["nodejs-20", "web", "python-3.11", "python3", "postgresql-16"]
run = "node dist/index.js"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["gmsh", "emscripten", "jq", "ffmpeg", "cmake", "lsof"]

[deployment]
deploymentTarget = "autoscale"
build = ["bash", "-lc", "VITE_HELIX_ASK_JOB_TIMEOUT_MS=1200000 npm run build && node scripts/deploy-clean.cjs && npm prune --omit=dev"]
run = ["bash", "-lc", "node dist/index.js"]

[[ports]]
localPort = 5000
externalPort = 80

